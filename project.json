{
    "name": "unfinity",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/unfinity",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/next:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "root": "apps/unfinity",
                "outputPath": "dist/apps/unfinity"
            },
            "configurations": {
                "development": {
                    "outputPath": "apps/unfinity"
                },
                "production": {}
            }
        },
        "serve": {
            "executor": "@nrwl/next:server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "unfinity:build",
                "dev": true
            },
            "configurations": {
                "development": {
                    "buildTarget": "unfinity:build:development",
                    "dev": true
                },
                "production": {
                    "buildTarget": "unfinity:build:production",
                    "dev": false
                }
            }
        },
        "export": {
            "executor": "@nrwl/next:export",
            "options": {
                "buildTarget": "unfinity:build:production"
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "apps/unfinity/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/unfinity/**/*.{ts,tsx,js,jsx}"]
            }
        }
    },
    "tags": []
}
